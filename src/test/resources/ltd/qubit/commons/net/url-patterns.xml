<url-patterns>
    <url-pattern>
        <part>PATH</part>
        <pattern type="PREFIX">http://www.sina.com.cn/news/</pattern>
    </url-pattern>

    <url-pattern>
        <pattern type="GLOB">http://www.sina.com.cn/*</pattern>
    </url-pattern>

    <url-pattern>
        <pattern type="SUFFIX">news.html</pattern>
    </url-pattern>

    <url-pattern>
        <part>HOSTNAME</part>
        <pattern type="REGEX">.*\.com$</pattern>
    </url-pattern>

    <url-pattern>
        <part>HOSTNAME</part>
        <pattern type="GLOB">*.com</pattern>
    </url-pattern>

    <url-pattern>
        <part>SCHEME</part>
        <pattern type="LITERAL">http</pattern>
    </url-pattern>

    <url-pattern>
        <part>SCHEME</part>
        <pattern>https</pattern>
    </url-pattern>
</url-patterns>
